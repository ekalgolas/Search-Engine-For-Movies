<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>SimpleServletProject</display-name>
  <servlet>
    <servlet-name>xmlServlet</servlet-name>
    <servlet-class>org.irproject.movies.XmlServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>JSONServlet</servlet-name>
    <servlet-class>org.irproject.movies.JSONServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QueryExpansionServlet</servlet-name>
    <servlet-class>org.irproject.movies.QueryExpansionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>xmlServlet</servlet-name>
    <url-pattern>/xmlServletPath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JSONServlet</servlet-name>
    <url-pattern>/JSONServletPath</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryExpansionServlet</servlet-name>
    <url-pattern>/QueryExpansionServlet</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>org.irproject.movies.Config</listener-class>
  </listener>
</web-app>